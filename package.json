{
  "name": "@serlo/database-layer-actix-workspace",
  "version": "0.0.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/serlo/serlo.org-database-layer/issues"
  },
  "repository": "serlo/serlo.org-database-layer",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "scripts": {
    "changelog": "ts-node scripts/changelog",
    "check:all": "scripts/run_all_checks.sh",
    "clippy": "cargo clippy --all-features -- -D warnings",
    "deploy": "ts-node scripts/deploy",
    "fetch": "./scripts/fetch.sh",
    "format": "prettier --write .",
    "mysql": "docker-compose exec mysql mysql --user=root --password=secret serlo",
    "mysql:dump": "ts-node scripts/mysql-dump",
    "mysql:import-anonymous-data": "ts-node scripts/mysql-import-anonymous-data",
    "mysql:rollback": "ts-node scripts/mysql-rollback",
    "pacts": "jest --config jest.config-pacts.js",
    "sqlx:prepare": "cargo sqlx prepare --merged",
    "start": "docker-compose up --detach",
    "version": "./scripts/update_server_version.sh"
  },
  "devDependencies": {
    "@inyono/changelog": "^0.5.2",
    "@pact-foundation/pact": "^9.17.3",
    "@types/node": "^17.0.23",
    "@types/ramda": "^0.28.7",
    "@types/semver": "^7.3.9",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "prettier-plugin-packagejson": "^2.2.17",
    "prettier-plugin-sh": "^0.10.0",
    "ramda": "^0.28.0",
    "semver": "^7.3.5",
    "toml": "^3.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "^16.0.0"
  },
  "packageManager": "yarn@3.2.0"
